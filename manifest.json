{
  "manifest_version": 3,
  "name": "URLWatcher",
  "version": "1",
  "description": "Detect Malicous URLs.",
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}



